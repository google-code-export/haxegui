-main Loader.hx
-lib feffects
-lib hscript
-swf9 loader.swf
-swf-version 9
-swf-header 960:600:300:3C3C3C
-debug

--next
-main App.hx
-cp ./
-cp ./controls

#-lib TweenerHX
-lib feffects
-lib hscript

-swf-lib assets.swf

### Default Styles ###

	# Button Style
	-resource haxegui/styles/DefaultButtonStyle.hscript@DefaultButtonStyle

	# CheckBox Style
	-resource haxegui/styles/DefaultCheckBoxStyle.hscript@DefaultCheckBoxStyle

	# RadioButton Style
	-resource haxegui/styles/DefaultRadioButtonStyle.hscript@DefaultRadioButtonStyle
	
	# ComboBox Style
	-resource haxegui/styles/DefaultComboBoxDropButton.hscript@DefaultComboBoxDropButton
	-resource haxegui/styles/DefaultComboBoxBackground.hscript@DefaultComboBoxBackground

	# Input
	-resource haxegui/styles/DefaultInputStyle.hscript@DefaultInputStyle

	# Window Style
	-resource haxegui/styles/DefaultWindowFrameStyle.hscript@DefaultWindowFrameStyle

	# Titlebar Style
	-resource haxegui/styles/DefaultTitleBarCloseButtonStyle.hscript@DefaultTitleBarCloseButtonStyle
	-resource haxegui/styles/DefaultTitleBarMinimizeButtonStyle.hscript@DefaultTitleBarMinimizeButtonStyle
	-resource haxegui/styles/DefaultTitleBarMaximizeButtonStyle.hscript@DefaultTitleBarMaximizeButtonStyle
	-resource haxegui/styles/DefaultTitleBarStyle.hscript@DefaultTitleBarStyle

	# Scrollbar Style
	-resource haxegui/styles/DefaultScrollbarUpButtonStyle.hscript@DefaultScrollbarUpButtonStyle
	-resource haxegui/styles/DefaultScrollbarDownButtonStyle.hscript@DefaultScrollbarDownButtonStyle
	-resource haxegui/styles/DefaultScrollbarHandleStyle.hscript@DefaultScrollbarHandleStyle

### Output ###

-swf9 main.swf
-swf-version 9
-swf-header 960:600:300:3C3C3C
--flash-use-stage
--flash-strict
-debug
-xml output.xml
