-main Loader.hx
-lib feffects
-lib hscript
-swf9 loader.swf
-swf-version 9
-swf-header 960:600:300:3C3C3C
-debug

--next
-main App.hx
-cp ./
-cp ./controls

#-lib TweenerHX
-lib feffects
-lib hscript

-swf-lib assets.swf

### Default Styles ###

	# Button Style
	-resource assets/styles/Default/ButtonStyle.hscript@DefaultButtonStyle

	# CheckBox Style
	-resource assets/styles/Default/CheckBoxStyle.hscript@DefaultCheckBoxStyle

	# RadioButton Style
	-resource assets/styles/Default/RadioButtonStyle.hscript@DefaultRadioButtonStyle
	
	# ComboBox Style
	-resource assets/styles/Default/ComboBoxDropButton.hscript@DefaultComboBoxDropButton
	-resource assets/styles/Default/ComboBoxBackground.hscript@DefaultComboBoxBackground

	# Input Style
	-resource assets/styles/Default/InputStyle.hscript@DefaultInputStyle
	
	# ListItem Style
	-resource assets/styles/Default/ListItemStyle.hscript@DefaultListItemStyle

	# Window Style
	-resource assets/styles/Default/WindowFrameStyle.hscript@DefaultWindowFrameStyle

	# Titlebar Style
	-resource assets/styles/Default/TitleBarCloseButtonStyle.hscript@DefaultTitleBarCloseButtonStyle
	-resource assets/styles/Default/TitleBarMinimizeButtonStyle.hscript@DefaultTitleBarMinimizeButtonStyle
	-resource assets/styles/Default/TitleBarMaximizeButtonStyle.hscript@DefaultTitleBarMaximizeButtonStyle
	-resource assets/styles/Default/TitleBarStyle.hscript@DefaultTitleBarStyle

	# Scrollbar Style
	-resource assets/styles/Default/ScrollbarUpButtonStyle.hscript@DefaultScrollbarUpButtonStyle
	-resource assets/styles/Default/ScrollbarDownButtonStyle.hscript@DefaultScrollbarDownButtonStyle
	-resource assets/styles/Default/ScrollbarHandleStyle.hscript@DefaultScrollbarHandleStyle

### Output ###

-swf9 main.swf
-swf-version 9
-swf-header 960:600:300:3C3C3C
--flash-use-stage
--flash-strict
-debug
-xml output.xml
